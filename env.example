# Database Configuration
MONGODB_URI=mongodb://localhost:27017/codeintervu

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here

# Cloudinary Configuration
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# Server Configuration
PORT=5000
NODE_ENV=development
# CORS allowed origins (comma-separated, only used in production)
ALLOWED_ORIGINS=https://codeintervu.com,https://www.codeintervu.com,https://admincodeintervu.netlify.app
# Allow localhost in production for testing (true/false)
ALLOW_DEV_ORIGINS=false
# Judge0 API Configuration
RAPIDAPI_KEY=your_rapidapi_key_here 